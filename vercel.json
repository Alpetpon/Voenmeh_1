{
  "version": 2,
  "builds": [
    {
      "src": "index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.py"
    }
  ],
  "functions": {
    "index.py": {
      "runtime": "python3.11",
      "maxDuration": 10
    }
  },
  "env": {
    "VERCEL": "1"
  }
} 
